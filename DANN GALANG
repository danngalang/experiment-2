// Experiment 2

#include <iostream>
#include <conio.h>
#include <string>

using namespace std;

int main ()
{
	float MASS;
	float DENSITY;

	cout << "Value of the mass in grams of the object:" << endl;
	cout << " " << endl;
	cin >> MASS;
	
	cout << " " << endl;
	cout << " " << endl;
	cout << " " << endl;

	cout << "Density in grams per cubic centimeters of the object: " << endl;
	cout << " " << endl;
	cin >> DENSITY;

	cout << " " << endl;
	cout << " " << endl;
	cout << " " << endl;

	cout << "Total volume of the object is :" << MASS / DENSITY << endl;

	_getch ();
	return 0;
}
